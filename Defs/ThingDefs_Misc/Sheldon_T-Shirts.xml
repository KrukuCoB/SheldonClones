<?xml version="1.0" encoding="utf-8" ?>
<Defs>

   <ThingDef Name="ApparelBase" Abstract="True">
        <thingClass>Apparel</thingClass>
		<category>Item</category>
		<selectable>True</selectable>
		<pathCost>10</pathCost>
		<useHitPoints>True</useHitPoints>
		<techLevel>Medieval</techLevel>
		<graphicData>
		    <onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
		</graphicData>
		<drawGUIOverlay>true</drawGUIOverlay>
		<statBases>
		    <MaxHitPoints>100</MaxHitPoints>
            <Flammability>0.9</Flammability>
            <DeteriorationRate>2</DeteriorationRate>
			<Beauty>1</Beauty>
	    </statBases>
		<altitudeLayer>Item</altitudeLayer>
		<alwaysHaulable>True</alwaysHaulable>
		<generateCommonality>0.5</generateCommonality>
	    <generateAllowChance>0.1</generateAllowChance>
		<tickerType>Never</tickerType>
		<burnableByRecipe>true</burnableByRecipe>
		<smeltable>true</smeltable>
		<comps>
            <li Class="CompProperties_Forbiddable"/>
            <li>
                <compClass>CompColorable</compClass>
            </li>
			<li>
                <compClass>CompQuality</compClass>
            </li>
    </comps>
	</ThingDef>
   
   <ThingDef Name="ShAmy" ParentName="ApparelBase" Abstract="True">
        <recipeMaker>
		    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Tailor</effectWorking>
			<soundWorking>Recipe_Tailor</soundWorking>
			<researchPrerequisite>ComplexClothing</researchPrerequisite>
			<recipeUsers Inherit="False">
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
			<skillRequirements>
			    <Crafting>2</Crafting>
			</skillRequirements>
		</recipeMaker>
		<tradeability>Sellable</tradeability>
    </ThingDef>
   
   
   
   <ThingDef ParentName="ShAmy">
		<defName>KC_GreenLantern</defName>
		<label>Green lantern</label>
		<description>Whaddaya get when you take one green alien and add one power ring? Why, The Green Lantern, of course! This is simple arithmetic, guys, and probably the most famous equation in history (aside from that whole e=mcÂ²).</description>
		<graphicData>
			<texPath>Things/Apparels/GreenLantern/GreenLantern</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<costStuffCount>35</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<statBases>
			<MaxHitPoints>125</MaxHitPoints>
			<WorkToMake>2900</WorkToMake>
			<Mass>0.35</Mass>
			<StuffEffectMultiplierArmor>0.02</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.5</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.3</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>1.5</EquipDelay>
		</statBases>
		<thingCategories Inherit='False'>
			<li>Apparel</li>
		</thingCategories>
		<apparel>
		    <countsAsClothingForNudity>true</countsAsClothingForNudity>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
			</bodyPartGroups>
